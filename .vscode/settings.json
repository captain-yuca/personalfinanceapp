{
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#ee505a",
    "activityBar.activeBorder": "#24e817",
    "activityBar.background": "#ee505a",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#24e817",
    "activityBarBadge.foreground": "#15202b",
    "statusBar.background": "#ea212e",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#ee505a",
    "titleBar.activeBackground": "#ea212e",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#ea212e99",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#EA212E",
  "prettier.requireConfig": true,
  "eslint.packageManager": "yarn",
  "prettier.jsxBracketSameLine": true,
  "prettier.printWidth": 120,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "files.associations": {
    "*.js": "javascriptreact",
    "*.jsx": "javascriptreact"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss]": {
    "editor.formatOnSave": false
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.alwaysShowStatus": true,
  "editor.formatOnSave": true,
  "eslint.format.enable": true,
  "editor.formatOnPaste": true
}